<h1>Please Enter Job Profile Here</h1>
<div id="show_item"></div>
<%=form_remote_tag :url => {:action => 'create' },
:update => "show_item",
:complete => visual_effect(:highlight, "show_item") %>
<%= error_messages_for :jobprofile %>
<p><label for ="jobprofiles_jobprofile">Job Profile</label>:
<%= text_field 'jobprofile', 'jobprofile' %></p>

<%= submit_tag 'create' %>
<%= link_to 'Back',{:action=>'Back'}%> <%= link_to 'Search Here',{:action=>'find'}%>



